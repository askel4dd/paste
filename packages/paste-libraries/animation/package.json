{
  "name": "@twilio-paste/animation-library",
  "version": "0.2.1",
  "category": "interaction",
  "status": "beta",
  "description": "Paste's animation library of choice.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c --environment NODE_ENV:production && yarn build:types",
    "build:dev": "yarn clean && rollup -c --environment NODE_ENV:development && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo && rm -rf .rpt2_cache",
    "prepublishOnly": "yarn build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-spring/shared": "9.0.0-rc.3",
    "@react-spring/web": "9.0.0-rc.3"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
